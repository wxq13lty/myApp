#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/gs/my_app/server/node_modules/.pnpm/clean-css@3.4.28/node_modules/clean-css/bin/node_modules:/mnt/d/gs/my_app/server/node_modules/.pnpm/clean-css@3.4.28/node_modules/clean-css/node_modules:/mnt/d/gs/my_app/server/node_modules/.pnpm/clean-css@3.4.28/node_modules:/mnt/d/gs/my_app/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/gs/my_app/server/node_modules/.pnpm/clean-css@3.4.28/node_modules/clean-css/bin/node_modules:/mnt/d/gs/my_app/server/node_modules/.pnpm/clean-css@3.4.28/node_modules/clean-css/node_modules:/mnt/d/gs/my_app/server/node_modules/.pnpm/clean-css@3.4.28/node_modules:/mnt/d/gs/my_app/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../clean-css@3.4.28/node_modules/clean-css/bin/cleancss" "$@"
else
  exec node  "$basedir/../../../../../clean-css@3.4.28/node_modules/clean-css/bin/cleancss" "$@"
fi
