#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/gs/my_app/server/node_modules/.pnpm/jade@1.11.0/node_modules/jade/bin/node_modules:/mnt/d/gs/my_app/server/node_modules/.pnpm/jade@1.11.0/node_modules/jade/node_modules:/mnt/d/gs/my_app/server/node_modules/.pnpm/jade@1.11.0/node_modules:/mnt/d/gs/my_app/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/gs/my_app/server/node_modules/.pnpm/jade@1.11.0/node_modules/jade/bin/node_modules:/mnt/d/gs/my_app/server/node_modules/.pnpm/jade@1.11.0/node_modules/jade/node_modules:/mnt/d/gs/my_app/server/node_modules/.pnpm/jade@1.11.0/node_modules:/mnt/d/gs/my_app/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/jade@1.11.0/node_modules/jade/bin/jade.js" "$@"
else
  exec node  "$basedir/../.pnpm/jade@1.11.0/node_modules/jade/bin/jade.js" "$@"
fi
